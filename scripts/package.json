{
  "name": "rover-scripts",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "deploy:osmosis": "yarn build && node build/deploy/osmosis",
    "generate-types": "yarn rust-schema && tsc --project codegen-tsconfig.json && rm -rf types/generated && node build/codegen && node build/codegen/insertIgnores.js && yarn format",
    "rust-schema": "cd ../ && cargo make generate-all-schemas && cd scripts",
    "compile-wasm": "cd ../ && cargo make rust-optimizer && cd scripts",
    "compile-hello-wasm-base": "wasm-pack build ../packages/hello-wasm --out-name index",
    "compile-hello-wasm-web": "yarn run compile-hello-wasm-base --target web --out-dir ../../scripts/health/pkg-web",
    "compile-hello-wasm-node": "yarn run compile-hello-wasm-base --target nodejs --out-dir ../../scripts/health/pkg-node",
    "copy-wasm": "copyfiles health/**/*.wasm ./build",
    "build": "tsc && yarn copy-wasm",
    "lint": "eslint . && yarn build && yarn format-check",
    "format": "prettier --write .",
    "format-check": "prettier --check ."
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.29.4",
    "@cosmjs/stargate": "^0.29.4",
    "@cosmwasm/ts-codegen": "^0.24.0",
    "chalk": "4.1.2",
    "copyfiles": "^2.4.1",
    "cosmjs-types": "^0.5.2",
    "lodash": "^4.17.21",
    "long": "^5.2.1",
    "prepend-file": "^2.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
